<h1>Listing mini_lectures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mini_lectures.each do |mini_lecture| %>
      <tr>
        <td><%= link_to 'Show', mini_lecture %></td>
        <td><%= link_to 'Edit', edit_mini_lecture_path(mini_lecture) %></td>
        <td><%= link_to 'Destroy', mini_lecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mini lecture', new_mini_lecture_path %>
